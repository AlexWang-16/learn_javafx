<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="511.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <LineChart fx:id="lineChart" layoutX="-10.0" prefHeight="438.0" prefWidth="511.0" title="My Line Chart">
        <xAxis>
          <CategoryAxis label="My X Label" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="My Y Label" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button layoutX="221.0" layoutY="449.0" mnemonicParsing="false" onAction="#loadChart" text="Load Chart" />
      <Label fx:id="label" contentDisplay="CENTER" layoutX="44.0" layoutY="405.0" prefHeight="58.0" prefWidth="127.0" textFill="#b20303">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
